{
  "version": 2,
  "name": "gh-photo.williamli.dev",
  "alias": [
    "gh-photo.williamli.dev"
  ],
  "regions": ["all"],
  "routes": [
    { 
      "src": "/", 
      "headers": {"cache-control": "public, max-age=14400"},
      "dest": "http://github.com/williamli.png"
    },
    {
      "src": "/(.*)",
      "headers": {"cache-control": "public, max-age=14400"},
      "dest": "http://github.com/williamli.$1"
    }
  ]
}